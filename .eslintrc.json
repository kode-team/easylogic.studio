{
    "plugins": ["jsx", "prettier"],
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "rules": {
        "prettier/prettier": "error",
        "jsx/uses-factory": [1, {"pragma": "JSX"}],
        "jsx/factory-in-scope": 0,
        "jsx/mark-used-vars": 1,
        "jsx/no-undef": 1,
        "no-redeclare": 0
    },
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "globals": {
        "window": true,
        "document": true,
        "console": true,
        "Proxy": true,
        "Promise": true,
        "Symbol": true,
        "__dirname": true,
        "Map": true,
        "Set": true
    }
}